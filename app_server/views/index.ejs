		<script>
			function ResizeHeight() {
				var screenHeight = $('.carousel').height();


				$('.slide').css({
					'height' : screenHeight + 'px'
				});

				$('.cell').css({
					'height' : (screenHeight / 2) + 'px'
				});
			}





			$(document).ready(function() {
				ResizeHeight();


				$('.carousel').flexslider({
					animation: "slide",
					animationLoop: true,
					directionNav: true,
					itemMargin: 0,
					slideshowSpeed: 15000,
					touch: true
				});
			});





			$(window).resize(function() {
				ResizeHeight();
			});
		</script>
